    "def convertToCSV(file,name):\n",
    "    name = name+\".csv\"\n",
    "    las = lasio.read(file)\n",
    "    df = las.df()\n",
    "    df.drop_duplicates(inplace=True)\n",
    "    df.to_csv(name)"
